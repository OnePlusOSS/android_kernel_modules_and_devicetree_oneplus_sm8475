&tlmm {
	ufcs_int_default: ufcs_int_default {
		mux {
			pins = "gpio7";
			function = "gpio";
		};
		config {
			pins = "gpio7";
			drive-strength = <2>;
			input-enable;
			bias-pull-up;
		};
	};

	ufcs_en_active: ufcs_en_active {
		mux {
			pins = "gpio6";
			function = "gpio";
		};
		config {
			pins = "gpio6";
			drive-strength = <2>;
			bias-pull-up;
			input-disable;
			output-high;
		};
	};

	ufcs_en_sleep: ufcs_en_sleep {
		mux {
			pins = "gpio6";
			function = "gpio";
		};
		config {
			pins = "gpio6";
			drive-strength = <2>;
			bias-pull-down;
			input-disable;
			output-low;
		};
	};
};

&qupv3_se1_i2c {
	status = "ok";
	qcom,clk-freq-out = <400000>;

	oplus_sc2201: sc8547-sc2201@72{
		status = "okay";
		compatible = "oplus,sc2201-ufcs";
		reg = <0x72>;

		oplus,ufcs_int-gpio = <&tlmm 7 0>;
		oplus,ufcs_en-gpio = <&tlmm 6 0>;

		pinctrl-names = "ufcs_int_default", "ufcs_en_active", "ufcs_en_sleep"/*, "default"*/;
		pinctrl-0 = <&ufcs_int_default>;
		pinctrl-1 = <&ufcs_en_active>;
		pinctrl-2 = <&ufcs_en_sleep>;
	};
};

