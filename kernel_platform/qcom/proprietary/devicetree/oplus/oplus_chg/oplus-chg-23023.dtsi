&battery_charger {
	compatible = "oplus,hal_sm8450";

	oplus,voocphy_bidirect_cp_support;
	oplus,chg_ops = "plat-pmic";
	qcom,pmic_ftm_vbat = "/sys/bus/spmi/drivers/pmic-spmi/0-00/c42d000.qcom,spmi:qcom,pmk8350@0:vadc@3100/iio:device0/in_voltage_pm8350b_vbat_sns_input";
